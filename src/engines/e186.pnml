/* E186(Traxx) switches */
switch(FEAT_TRAINS, SELF, switch_e186_articulated, extra_callback_info1) {
    1: return item_dummy_2;        // dummy parts
    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}
switch(FEAT_TRAINS, SELF, switch_e186_visual_effect_local, position_in_vehid_chain % 2) {
    0: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 3, DISABLE_WAGON_POWER);
    return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
}
switch (FEAT_TRAINS, SELF, switch_e186_add_text, IS_HEATING_CLIMATE ) {
	1: return string(STR_PURCHASE_DESCR_3L, string(STR_HEATING_ELECTRIC_POWER_TEXT, HEATING_POWER_ELECTRIC_HI), string(STR_E186_TEXT), string(STR_ENGINE_FREIGHT));
	return string(STR_PURCHASE_DESCR_2L, string(STR_E186_TEXT), string(STR_ENGINE_FREIGHT));
}
ENGINE_RUNNING_COST_SWITCH(e186, E186)
ELECTRIC_HI_ENGINE_POWER_SWITCH(e186, KW_TO_HP(5600))
ENGINE_VISUAL_EFFECT_SWITCH(e186)

switch(FEAT_TRAINS, SELF, switch_e186_can_attach, vehicle_type_id) {
    item_twindexx:   return string(STR_ATTACH_DISALLOW, string(STR_E186_NAME));
    item_twindexxcc: return string(STR_ATTACH_DISALLOW, string(STR_E186_NAME));
    return switch_normal_can_attach;
}

/* Define the actual train */
item(FEAT_TRAINS, item_e186, 900) {
    property {
		/* common properties */
        name:                           string(STR_E186_NAME);
        climates_available:             PKP_CLIMATES; 
        introduction_date:              date(2008, 1, 1);
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life:                   30;
        reliability_decay:              20;
        CARGO_LOCOMOTIVES_DEFINITION
        loading_speed:                  5; 
        cost_factor:                    PURCHASE_COST_E186;
        running_cost_factor:            RUNNING_COST_E186;
		/* train properties */
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          140 km/h;
        misc_flags:                     bitmask();
        refit_cost:                     0;
        track_type:                     ELRL;
        ai_special_flag:                AI_FLAG_CARGO;
        power:                          5600 kW;
        running_cost_base:              RUNNING_COST_ELECTRIC;
        dual_headed:                    0;
        ENGINE_CAPACITY_PROPERTY
        weight:                         85 ton;
        ai_engine_rank:                 0; // not intended to be used by the ai
        engine_class:                   ENGINE_CLASS_ELECTRIC;
        extra_power_per_wagon:          0 kW;
        tractive_effort_coefficient:    0.360;
        air_drag_coefficient:           0.07;
        length:                         8;
        extra_weight_per_wagon:         0;
        visual_effect_and_powered:      visual_effect_and_powered(VISUAL_EFFECT_DEFAULT, 0, DISABLE_WAGON_POWER);
        bitmask_vehicle_info:           0;
    }

    graphics {
        purchase:                   sset_e186_purchase;
        additional_text:            switch_e186_add_text;
        default:                    sset_e186;
        articulated_part:           switch_e186_articulated;
        visual_effect_and_powered:  switch_e186_visual_effect;
        can_attach_wagon:           switch_e186_can_attach;
        start_stop:                 switch_normal_start_stop;
        ENGINE_CAPACITY_CALLBACKS
        ENGINE_RUNNING_COST_CALLBACKS(e186)
        ENGINE_POWER_CALLBACKS(e186)
    }
}
